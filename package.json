  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev:frontend": "npm --prefix frontend run dev",
    "dev:backend": "sleep 3; npm --prefix backend run dev",
    "dev": "concurrently 'npm:dev:frontend' 'npm:dev:backend'",
    "install:frontend": "npm --prefix frontend install",
